<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template inherit_id="qweb_pdf_report_example.sub_patients_personal_info_template" id="sub_patients_personal_info_template_entend">
        <xpath expr="//div[hasclass('page')]" position="replace">
            <div class="page">
                <h2>Extended Patient Information</h2>
                <p>Additional details about the patient can be found here.</p>
                <p>Name: <span t-field="doc.name"/></p>
                <p>Email: <span t-field="doc.email"/></p>
            </div>
        </xpath>
    </template>

</odoo>
